<?xml version="1.0" encoding="US-ASCII"?>

<!--Author: Andreas Sauter
    Bayesian network in XMLBIF v0.3 (BayesNet Interchange Format)
 -->

<!-- DTD for the XMLBIF 0.3 format -->
<!DOCTYPE BIF [
	<!ELEMENT BIF ( NETWORK )*>
	      <!ATTLIST BIF VERSION CDATA #REQUIRED>
	<!ELEMENT NETWORK ( NAME, ( PROPERTY | VARIABLE | DEFINITION )* )>
	<!ELEMENT NAME (#PCDATA)>
	<!ELEMENT VARIABLE ( NAME, ( OUTCOME |  PROPERTY )* ) >
	      <!ATTLIST VARIABLE TYPE (nature|decision|utility) "nature">
	<!ELEMENT OUTCOME (#PCDATA)>
	<!ELEMENT DEFINITION ( FOR | GIVEN | TABLE | PROPERTY )* >
	<!ELEMENT FOR (#PCDATA)>
	<!ELEMENT GIVEN (#PCDATA)>
	<!ELEMENT TABLE (#PCDATA)>
	<!ELEMENT PROPERTY (#PCDATA)>
]>

<BIF VERSION="0.3">
<NETWORK>
    <NAME>Berlin Clubs</NAME>

    <!-- Variables-->
    <VARIABLE TYPE="nature">
        <NAME>Tourist</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Wasted</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>DJ</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Enter at Night</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Guestlist</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Leather Outfit</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Piercings</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Queer Looks</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Enter Berghain</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>Enter Matrix</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>

    <!-- PCTs -->
    <DEFINITION>
        <FOR>Tourist</FOR>
        <TABLE>0.97 0.03</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Leather Outfit</FOR>
        <TABLE>0.95 0.05</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Piercings</FOR>
        <TABLE>0.9 0.1</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Wasted</FOR>
        <GIVEN>Tourist</GIVEN>
        <TABLE>0.6 0.4 0.25 0.75</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>DJ</FOR>
        <GIVEN>Wasted</GIVEN>
        <TABLE>0.6 0.4 0.7 0.3 </TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Enter at Night</FOR>
        <GIVEN>Tourist</GIVEN>
        <TABLE>0.9 0.1 0.3 0.7</TABLE> <!-- Order? Rain?, Sprinkler?, Wet Grass?-->
    </DEFINITION>
    <DEFINITION>
        <FOR>Guestlist</FOR>
        <GIVEN>DJ</GIVEN>
        <GIVEN>Tourist</GIVEN>
        <TABLE> 0.9 0.1 0.99 0.01 0.1 0.9 0.7 0.3 </TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Queer Looks</FOR>
        <GIVEN>Leather Outfit</GIVEN>
        <GIVEN>Piercings</GIVEN>
        <TABLE>0.8 0.2 0.6 0.4 0.2 0.8 0.1 0.9</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Enter Berghain</FOR>
        <GIVEN>Enter at Night</GIVEN>
        <GIVEN>Wasted</GIVEN>
        <GIVEN>Queer Looks</GIVEN>
        <GIVEN>Guestlist</GIVEN>
        <TABLE> 0.65 0.35 0.75 0.25 0.85 0.15 0.95 0.05 0.2 0.8 0.3 0.7 0.6 0.4 0.7 0.3 0.05 0.95 0.15 0.85 0.55 0.45 0.6 0.4 0.05 0.95 0.1 0.9 0.4 0.6 0.5 0.5</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>Enter Matrix</FOR>
        <GIVEN>Enter at Night</GIVEN>
        <GIVEN>Wasted</GIVEN>
        <GIVEN>Tourist</GIVEN>
        <GIVEN>Guestlist</GIVEN>
        <TABLE> 1 0 0.1 0.9 1 0 0.35 0.65 1 0 0.2 0.8 1 0 0.4 0.6 1 0 0.05 0.95 1 0 0.15 0.85 1 0 0.1 0.9 1 0 0.2 0.8</TABLE>
    </DEFINITION> 
</NETWORK>
</BIF>